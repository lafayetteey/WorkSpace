package com.test02;

public class Student {

	public static void main(String[] args) {
		String name = "김승진";
		int korea = 90;
		int eng = 80;
		int math = 75;
		
		Score sc = new Score();
		
		//Score클래스 메소드에서 선언된 리턴 타입인 double avg에 값을 초기화 한 후 출력
